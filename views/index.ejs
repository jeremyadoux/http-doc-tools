<!DOCTYPE html>
<html>
  <head>
    <title></title>
    <link rel='stylesheet' href='/stylesheets/ui.dynatree.css' />
    <link rel='stylesheet' href='/stylesheets/base.css' />
    <link rel='stylesheet' href='/stylesheets/hnd.css' />
    <link rel='stylesheet' href='/stylesheets/jquery-ui-1.8.12.custom.css' />
    <link rel='stylesheet' href='/stylesheets/reset.css' />
    <link rel='stylesheet' href='/stylesheets/toc.css' />
    <link rel='stylesheet' href='/stylesheets/bootstrap.min.css' />
    <link rel='stylesheet' href='/stylesheets/custom.css' />

    <script type="text/javascript" src="/javascripts/jquery.min.js"></script>
    <script type="text/javascript" src="/javascripts/jquery-ui-1.8.17.custom.min.js"></script>
    <script type="text/javascript" src="/javascripts/jquery.cookie.js"></script>
    <script type="text/javascript" src="/javascripts/jquery.dynatree.min.js"></script>
    <script type="text/javascript" src="/javascripts/hndjsse.js"></script>
    <script type="text/javascript" src="/javascripts/custom.js"></script>

    <script type="text/javascript">
        $(document).ready(function() {
            var lang = '<%= lang %>';

            if(lang === 'none')
            {
                lang = getUserLanguage();
                window.location.replace(lang+'/<%- page %>');
            }

            var langClass = '.<%- lang %>';

            $(langClass).addClass("selected");
            document.title=$('#topic_header_text')[0].innerHTML;
        });
    </script>


  </head>
  <body>
  <div class="row">
    <div class="col-md-4" id="left">
        <%if(lang != 'none') {%>
            <%- include('doc/'+lang+'/toc.ejs');%>
        <%} %>
    </div>
    <div class="col-md-8" id="right">
      <a href="/fr/<%- page.replace('ejs', 'html') %>" onclick="return switchLanguage('fr');"><img src="/img/fr-flag.png" class="fr"/></a>
      <a href="/en/<%- page.replace('ejs', 'html') %>" onclick="return switchLanguage('en');"><img src="/img/en-flag.png" class="en"/></a>
        <%if(lang != 'none') {%>
            <%-include('doc/' + lang +'/' + page);%>
        <%} %>
    </div>
  </div>
  </body>
</html>
